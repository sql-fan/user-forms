<%= form_with model: @user do |form| %>
  <div class="">
    <%= form.label :username %>
    <%= form.text_field :username %>
  </div>
  <div class="">
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>
  <div class="">
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  <%= form.submit %>
<% end %>
